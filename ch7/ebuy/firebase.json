{
    "hosting": [
        {
            "target": "app-shell",
            "public": "dist/apps/app-shell",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "headers": [
              {
                "source": "**/*.@(eot|otf|ttf|ttc|woff|js|font.css|remoteEntry.js)",
                "headers": [
                  {
                    "key":"Access-Control-Allow-Origin",
                    "value":"https://ebuy-20fb3.web.app"
                  },
                  {
                    "key":"Cache-Control",
                    "value":"no-store, max-age=0"
                  }
                ]
              }
            ]
        },
